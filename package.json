    "sass": "^1.52.1",
    "secp256k1": "^4.0.3",
    "semver": "^7.3.7",
    "sentence-case": "^3.0.4",
    "xss": "1.0.11"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/numeral": "^2.0.2",
    "@types/qrcode.react": "^1.0.2",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.13",
    "@types/react": "^18.0.9",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^18.0.5",
    "@types/react-modal": "^3.13.1",
