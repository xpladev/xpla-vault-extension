{
  "name": "xpla-vault-extension",
  "version": "1.7.0",
  "hompage": "https://www.xpla.io",
  "author": "XPLA",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "export PORT=9020 && react-scripts start",
    "build-scripts": "webpack --config scripts/webpack.config.js",
    "build:dev": "cp .env.development .env.production && cross-env NODE_ENV=production react-app-rewired build && npm run build-scripts",
    "build:stage": "cp .env.stage .env.production && cross-env NODE_ENV=production react-app-rewired build && npm run build-scripts",
    "build:product": "cp .env.product .env.production && cross-env NODE_ENV=production react-app-rewired build && npm run build-scripts",
    "test": "react-scripts test",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint --fix src --ext .ts,.tsx"
  },
  "dependencies": {
    "@ethersproject/bytes": "^5.6.1",
    "@ledgerhq/hw-transport-web-ble": "^6.27.1",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.8.1",
    "@tippyjs/react": "^4.2.6",
    "@xpla.kitchen/utils": "^0.2.2",
    "@xpla/ledger-xpla-js": "^1.7.0-beta.4",
    "@xpla/log-finder-ruleset": "^1.7.0-beta.2",
    "@xpla/msg-reader": "^1.7.0-beta.1",
    "@xpla/wallet-provider": "^1.7.1-beta.3",
    "@xpla/xpla.js": "^1.7.0-beta.3",
    "@xpla/xpla.proto": "^1.7.0-beta.1",
    "axios": "^1.7.9",
    "bignumber.js": "^9.1.2",
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.28.0",
    "eip55": "^2.1.1",
    "i18next": "^21.8.4",
    "i18next-browser-languagedetector": "^6.1.4",
    "immutability-helper": "^3.1.1",
    "js-base64": "^3.7.2",
    "keccak256": "^1.0.6",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "qrcode.react": "^3.0.2",
    "qs": "^6.10.3",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "7.22.5",
    "react-i18next": "^11.16.9",
    "react-modal": "^3.15.1",
